{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Echo",
  "colors": {
    "primary": "#4674b8",
    "light": "#4674b8",
    "dark": "#4674b8"
  },
  "favicon": "/favicon.png",
  "navigation": {
    "groups": [
      {
        "group": "Overview",
        "pages": ["overview/introduction", "overview/donator-perks"]
